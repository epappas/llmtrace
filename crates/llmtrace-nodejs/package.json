    "cost-estimation",
    "napi",
    "native"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/example/llmtrace"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0",
    "typescript": "^5.8.0"
  }
