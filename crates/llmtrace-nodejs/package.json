{
  "name": "@llmtrace/node",
  "version": "0.1.0",
  "description": "Native Node.js bindings for LLMTrace â€” LLM security analysis, tracing, and cost estimation",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
